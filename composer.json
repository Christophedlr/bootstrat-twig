{
    "name": "christophedlr/bootstrap-twig",
    "description": "Bootstrap-Twig is an library for add Twig Extension for manage Bootstrap",
    "type": "library",
    "keywords": ["templating"],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.5",
        "twig/twig": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "9.1.2",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Bootstrap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Daloz - De Los Rios",
            "email": "christophedlr@gmail.com",
            "role": "Lead Developer"
        }
    ]
}
